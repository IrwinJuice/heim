{
  "deploy": {
    "root_path": "D:\\projects\\artifacts",
    "artifacts": [
      {
        "id": "example-service",
        "kind": "java",
        "backup": true,
        "destination": "example-service",
        "excluded_files": [
          "up.ps1"
        ],
        "run_after": {
          "powershell": "Start-Process powershell -ArgumentList 'D:\\projects\\artifacts\\example-service\\up.ps1'"
        }
      },
      {
        "id": "example-client",
        "kind": "angular",
        "backup": true,
        "destination": "example-client\\example"
      }
    ]
  }
}
